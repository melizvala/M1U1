<div class="col-md-4 mt-4 mt-sm-0" id="contactanos" class="contactanos" class="container-fluid">
  <h2>Contactanos</h2>
  <form action="/" method="post">
    <div class="form-group col-md-6">
      <label for="nombre"><p>Usuario</p></label>
      <input type="text" class="form-control" name="usuario" id="nombre" placeholder="nombre de usuario">
    </div>
    <div class="form-group">
      <label for="email"><p>Email</p></label>
      <input type="email" name="email" class="form-control" id="email" aria-describedby="emailHelp">
      <small id="emailHelp" class="form-text text-muted"><p>We'll never share your email with anyone else.</p></small>
    </div>
    <div class="form-group">
      <label for="Mensaje"><p>Mensaje</p></label>
      <textarea class="form-control" name="mensaje" id="mensaje" rows="4"></textarea>
    </div>
    <button type="submit" class="btn btn-primary">Enviar</button>
  </form>
  {{#if message}}
{{message}}
{{/if}}
</div>